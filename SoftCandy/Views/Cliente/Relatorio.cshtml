@model IEnumerable<SoftCandy.Models.Cliente>

@{
    ViewData["Title"] = "Clientes";
    Layout = "_LayoutRelatorio";
}
<h2>Clientes</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomeCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CelularCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LogradouroCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumeroCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BairroCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CidadeCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoCliente)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomeCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CelularCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LogradouroCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumeroCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BairroCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CidadeCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoCliente)
            </td>
        </tr>
        }
    </tbody>
</table>
<script>window.print()</script>