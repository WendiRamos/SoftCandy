@model IEnumerable<SoftCandy.Models.Cliente>

@{
    ViewData["Title"] = "Clientes";
    Layout = "_LayoutRelatorio";
}
<h2>Clientes</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomeCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CelularCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EnderecoCliente)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdCliente)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NomeCliente)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CelularCliente)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EnderecoCliente)
                </td>
            </tr>
        }
    </tbody>
</table>
<script>window.print()</script>